<form method="post" action="{{ path('admin.voiture.delete', {'id': voiture.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ voiture.id) }}">
    <button type="submit" class="btn btn-danger">Supprimer</button>
</form>
